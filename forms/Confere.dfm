  object SqlMovimentacaoExterna: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'ID_INTERNO'
        ParamType = ptInput
        Value = 0
      end>
    SQL.Strings = (
      'SELECT '
      '    T.DATA as "Dt movimenta'#231#227'o",'
      '    U.nome_up as "Origem",'
      
        '    IIF(T.TIPO_DESTINO<>'#39'U'#39', '#39'LOCAL: '#39'||D.DESTINO, U2.NOME_UP) a' +
        's "Destino",'
      '    T.TIPO_DOCUMENTO as "Tipo Doc",'
      '    T.NUMERO_DOCUMENTO as "N. Doc",'
      '    T.MOTIVO_MOVIMENTACAO as "Motivo da movimenta'#231#227'o"'
      'FROM TRANSFERENCIA_INTERNO T'
      
        '   INNER JOIN vinc_transferencia_interno V ON (V.id_transferenci' +
        'a_interno=T.id_transferencia_interno)'
      '   INNER JOIN INTERNO I ON (V.id_interno=I.id_interno)'
      '   LEFT JOIN DESTINO D ON (D.ID_DESTINO = T.ID_DESTINO)'
      '   LEFT JOIN UNIDADE_PENAL U ON (T.ID_ORIGEM = U.ID_UP)'
      '   LEFT JOIN UNIDADE_PENAL U2 ON (T.ID_UP_DESTINO = U2.ID_UP)'
      'where v.ID_INTERNO = :ID_INTERNO'
      'ORDER BY T.DATA DESC')
    SQLConnection = Dm.Conexao
    Left = 688
    Top = 80
  end
