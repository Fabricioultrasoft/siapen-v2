    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP&#13;&#10;  " Database="Database" PropData="05704C656674039C000470546F700388000A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="sqldeficiencias" UserName="sqldeficiencias" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select i.rgi,i.nome_interno, i.id_up,&#13;&#10;d.deficiencia,&#13;&#10;di.membro,di.tipo_locomocao,di.tipo_atrofia,di.motivo,&#13;&#10;pavilhao.pavilhao,galeria.galeria,solario.solario,cela.cela&#13;&#10;from deficiencia_interno di&#13;&#10;join interno i on (di.id_interno=i.id_interno)&#13;&#10;join deficiencia d on (d.id_deficiencia=di.id_deficiencia)&#13;&#10;INNER JOIN PAVILHAO ON (i.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;INNER JOIN GALERIA ON (i.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;INNER JOIN CELA ON (i.IDCELA = CELA.ID_CELA)&#13;&#10;INNER JOIN SOLARIO ON (i.IDSOLARIO = SOLARIO.ID_SOLARIO)  &#13;&#10;where  I.ID_UP = :ID_UP AND&#13;&#10;d.deficiencia LIKE '%'|| :deficiencia ||'%'&#13;&#10;order by i.nome_interno         " Database="Database" PropData=""/>
