    <TfrxDBXQuery Name="SqlTransfUP" UserName="SqlTransfUP" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT ti.id_up_destino, up.nome_up, count(ti.id_up_destino) as qtd&#13;&#10;&#13;&#10;FROM transferencia_interno ti&#13;&#10;INNER JOIN vinc_transferencia_interno vti ON (vti.id_transferencia_interno = ti.id_transferencia_interno)&#13;&#10;INNER JOIN unidade_penal up ON (up.id_up = ti.id_up_destino)&#13;&#10;&#13;&#10;WHERE ti.tipo_destino = 'U'&#13;&#10;AND ti.liberada = 'S'&#13;&#10;AND ti.recebida = 'S'&#13;&#10;AND ti.id_origem = :id_up&#13;&#10;AND ti.data BETWEEN :dataInicial and :dataFinal                                 &#13;&#10;&#13;&#10;GROUP BY ti.id_up_destino, up.nome_up&#13;&#10;ORDER BY up.nome_up                                                                                               " Database="database" PropData=""/>
