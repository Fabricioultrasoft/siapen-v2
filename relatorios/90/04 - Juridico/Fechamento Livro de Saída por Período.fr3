    <TfrxDBXQuery Name="SqlTransfOutros" UserName="SqlTransfOutros" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT ti.id_destino, d.destino, count(ti.id_destino) as qtd&#13;&#10;&#13;&#10;FROM transferencia_interno ti&#13;&#10;INNER JOIN vinc_transferencia_interno vti ON (vti.id_transferencia_interno = ti.id_transferencia_interno)&#13;&#10;INNER JOIN destino d ON (d.id_destino = ti.id_destino)&#13;&#10;&#13;&#10;WHERE ti.tipo_destino = 'O'&#13;&#10;AND ti.liberada = 'S'&#13;&#10;AND ti.recebida = 'S'&#13;&#10;AND ti.id_origem = :id_up&#13;&#10;AND ti.data BETWEEN :dataInicial and :dataFinal    &#13;&#10;&#13;&#10;GROUP BY ti.id_destino, d.destino&#13;&#10;ORDER BY d.destino                                                                                               " Database="database" PropData=""/>
