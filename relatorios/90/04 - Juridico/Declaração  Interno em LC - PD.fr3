    <TfrxDBXQuery Name="sqlinterno" UserName="sqlinterno" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;    i.rgi,&#13;&#10;    i.pai,&#13;&#10;    i.mae,&#13;&#10;    i.endereco,&#13;&#10;    i.bairro,&#13;&#10;    i.numero,&#13;&#10;    i.complemeneto, &#13;&#10;    i.nome_interno,  &#13;&#10;    d.destino,&#13;&#10;       (SELECT list(p.numero_inquerito, ' - ') as inqueritos&#13;&#10;       from PROCESSO p&#13;&#10;       where p.idinterno = i.id_interno)  &#13;&#10;&#13;&#10;FROM transferencia_interno t&#13;&#10;inner join vinc_transferencia_interno v on (v.id_transferencia_interno=t.id_transferencia_interno)&#13;&#10;inner join interno i on (i.id_interno=v.id_interno)&#13;&#10;&#13;&#10;left join destino d on (d.id_destino=t.id_destino)&#13;&#10;&#13;&#10;WHERE ID_UP = :ID_UP and ((d.destino like '%DOMICILIAR%') or (D.destino LIKE '%CONDICIONAL%'))  &#13;&#10;and i.nome_interno = :NOMEINTERNO        " Database="Database" PropData=""/>
