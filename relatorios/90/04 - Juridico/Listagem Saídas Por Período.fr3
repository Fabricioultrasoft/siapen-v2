    <TfrxDBXQuery Name="SqlConfere" UserName="SqlConfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT i.nome_interno, up.nome_up, d.destino, ti.motivo_movimentacao, ti.data, i.rgi                                                      &#13;&#10;&#13;&#10;FROM transferencia_interno ti&#13;&#10;INNER JOIN vinc_transferencia_interno vti ON (vti.id_transferencia_interno = ti.id_transferencia_interno)&#13;&#10;LEFT JOIN unidade_penal up ON (up.id_up = ti.id_up_destino)&#13;&#10;LEFT JOIN destino d ON (d.id_destino = ti.id_destino)&#13;&#10;INNER JOIN interno i ON (i.id_interno = vti.id_interno)&#13;&#10;&#13;&#10;WHERE ti.liberada = 'S'&#13;&#10;AND ti.recebida = 'S'&#13;&#10;AND ti.id_origem = :id_up&#13;&#10;AND ti.data BETWEEN :dataInicial and :dataFinal&#13;&#10;                 &#13;&#10;&#13;&#10;ORDER BY ti.data, i.nome_interno                                                                                             " Database="database" PropData=""/>
