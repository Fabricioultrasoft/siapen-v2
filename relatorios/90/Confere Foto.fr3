    <TfrxDBXQuery Name="sqlconfere" UserName="sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.RGI,&#13;&#10;    INTERNO.foto,&#13;&#10;    INTERNO.data_nascimento,&#13;&#10;    INTERNO.mae,&#13;&#10;    INTERNO.pai,&#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO&#13;&#10;&#13;&#10;FROM INTERNO&#13;&#10;   &#13;&#10;   INNER JOIN PAVILHAO ON (INTERNO.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;  &#13;&#10;WHERE INTERNO.ST = 'A' AND INTERNO.ID_UP = :ID_UP  AND  GALERIA.GALERIA || ' - ' || SOLARIO.SOLARIO || ' - ' || CELA.CELA LIKE '%' || :CELA || '%'  &#13;&#10;&#13;&#10;      " Database="database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="database" PropData="05704C6566740314010470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="sqlgaleria" UserName="sqlgaleria" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;&#13;&#10;    GALERIA.GALERIA || ' - ' || SOLARIO.SOLARIO || ' - ' || CELA.CELA as galeria                       &#13;&#10;&#13;&#10;FROM PAVILHAO&#13;&#10;   &#13;&#10;   INNER JOIN GALERIA ON (PAVILHAO.id_pavilhao=GALERIA.idpavilhao)&#13;&#10;   INNER JOIN SOLARIO ON (GALERIA.id_galeria = SOLARIO.idgaleria)&#13;&#10;   INNER JOIN CELA ON (SOLARIO.id_solario = CELA.idsolario)&#13;&#10;&#13;&#10;WHERE pavilhao.id_up =:ID_UP&#13;&#10;order by galeria                                    " Database="database" PropData="05704C6566740314010470546F7002640A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
