    <TfrxDBXQuery Name="SqlResumoGeral" UserName="SqlResumoGeral" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT&#13;&#10;    TRIM(PAVILHAO.PAVILHAO) PAVILHAO,&#13;&#10;    TRIM(GALERIA.GALERIA) GALERIA,&#13;&#10;    TRIM(SOLARIO.SOLARIO) SOLARIO,  &#13;&#10;    TRIM(CELA.CELA) CELA,  &#13;&#10;    TRIM(UNIDADE_PENAL.SIGLA) SIGLA                                                                                                     &#13;&#10;FROM INTERNO&#13;&#10;   INNER JOIN PAVILHAO ON (INTERNO.IDPAVILHAO = PAVILHAO.ID_PAVILHAO)&#13;&#10;   INNER JOIN UNIDADE_PENAL ON (UNIDADE_PENAL.ID_UP = PAVILHAO.ID_UP)                                                                                                                                                                        &#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;WHERE INTERNO.ST = 'A'&#13;&#10;AND COALESCE(INTERNO.EM_TRANSITO,'N')='N'    &#13;&#10;AND INTERNO.ID_UP = :ID_UP&#13;&#10;ORDER BY 1,2,3,4    &#13;&#10;  " Database="Database" PropData="05704C65667402600470546F7003D4000A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
