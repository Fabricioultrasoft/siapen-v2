    <TfrxDBXQuery Name="sqlvisitantesDia" UserName="sqlvisitantesDia" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;        v.visitante, iif(mdv.sexo='F','Feminino','Masculino') sexo,&#13;&#10;        p.pavilhao, mdv.quantidade_menor, i.nome_interno, v.tipo_visitante                                      &#13;&#10;from mov_diario_visitantes mdv&#13;&#10;join visitante_interno vi on (mdv.id_visitante=vi.id_visitante)&#13;&#10;join visitante v on (v.id_visitante=vi.id_visitante)&#13;&#10;join interno i on (vi.id_interno=i.id_interno)&#13;&#10;join pavilhao p on (i.idpavilhao=p.id_pavilhao)&#13;&#10;where mdv.data_visita = :DATA and i.id_up = :id_up                                   &#13;&#10;order by v.visitante&#13;&#10;&#13;&#10;&#13;&#10;     " Database="database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax, u.email,u.sigla                         &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="database" PropData="05704C6566740314010470546F7002240A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="sqldiretor" UserName="sqldiretor" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT f.nome_funcionario,f.funcao&#13;&#10;&#13;&#10;FROM funcionario  f&#13;&#10;&#13;&#10;where f.id_up = :up and f.funcao like '%DIRETOR%'  " Database="database" PropData="05704C6566740360010470546F7003C4000A506172616D657465727301010C3C000000204E616D653D227570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="sqlmenores" UserName="sqlmenores" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;        sum(iif(mdv.sexo = 'F', 1,0)) as totalfeminino,&#13;&#10;        sum(iif(mdv.sexo = 'M', 1,0)) as totalmasculino,                              &#13;&#10;        sum(iif(v.tipo_visitante = 'Menor', 1,0)) as totalmenor                             &#13;&#10;from mov_diario_visitantes mdv&#13;&#10;join visitante_interno vi on (mdv.id_visitante=vi.id_visitante)&#13;&#10;join visitante v on (v.id_visitante=vi.id_visitante)&#13;&#10;join interno i on (vi.id_interno=i.id_interno)&#13;&#10;join pavilhao p on (i.idpavilhao=p.id_pavilhao)&#13;&#10;where mdv.data_visita = :DATA and i.id_up = :id_up                                   &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;  " Database="database" PropData=""/>
