    <TfrxIBXQuery Name="sqlescolaridade" UserName="sqlescolaridade" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;I.nome_interno,&#13;&#10;e.escolaridade,&#13;&#10;p.pavilhao,&#13;&#10;g.galeria,&#13;&#10;c.cela,&#13;&#10;s.solario                    &#13;&#10;from interno i&#13;&#10;INNER JOIN escolaridade e ON (I.IDescolaridade = e.ID_escolaridade)        &#13;&#10;INNER JOIN PAVILHAO p ON (I.IDPAVILHAO = P.ID_PAVILHAO)&#13;&#10;iNNER JOIN GALERIA g ON (I.IDGALERIA = G.ID_GALERIA)&#13;&#10;INNER JOIN CELA c ON (I.IDCELA = C.ID_CELA)&#13;&#10;INNER JOIN SOLARIO s ON (I.IDSOLARIO = S.ID_SOLARIO)&#13;&#10;where i.st ='A' AND I.ID_UP = :ID_UP and  &#13;&#10;e.escolaridade LIKE :escolaridade || '%'&#13;&#10;order by e.escolaridade, i.nome_interno                                                 &#13;&#10;    " Database="IBXDatabase1" PropData=""/>
