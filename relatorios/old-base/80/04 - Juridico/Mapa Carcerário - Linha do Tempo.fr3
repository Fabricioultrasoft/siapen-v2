    <TfrxDBXQuery Name="sqlfichadisciplinar" UserName="sqlfichadisciplinar" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;i.nome_interno,  i.rgi, i.data_entrada, i.mae, i.pai,i.outro_nome,                                      &#13;&#10;i.endereco, i.numero, i.bairro, i.complemeneto, i.cep, i.data_nascimento, i.foto,             &#13;&#10;i.foto_perfil, i.ESTADO_CIVIL, na.cidade as naturalidade, na.uf as ufnaturalidade,&#13;&#10;n.nacionalidade, e.escolaridade, p.proedencia, c.cidade, c.uf, i.SINAIS_PARTICULARES,              &#13;&#10;pr.profissao, i.data_prisao, i.vulgo, r.raca, u.nome_up, I.condenacao,&#13;&#10;i.altura, i.peso, h.descricao as historico, &#13;&#10;h.data_hora as datahistorico, i.artigo, I.cpf, i.rg, i.nome_social, i.identidade_genero,&#13;&#10;i.orientacao_sexual, i.numero_infopen                                                                             &#13;&#10;from interno i&#13;&#10;left JOIN cidade na on (i.idnaturalidade = na.id_cidade)&#13;&#10;left JOIN cidade c on (i.idcidade = c.id_cidade)    &#13;&#10;left JOIN nacionalidade n ON (i.idnacionalidade = n.id_nacionalidade)&#13;&#10;left JOIN escolaridade e on (i.idescolaridade = e.id_escolaridade)&#13;&#10;left JOIN procedencia p ON (i.id_procedencia = p.id_procedencia)&#13;&#10;left JOIN profissao pr ON (i.idprofissao = pr.id_profissao)&#13;&#10;left JOIN raca r ON (i.idraca = r.id_raca)&#13;&#10;left JOIN unidade_penal u ON (i.id_up = u.id_up)&#13;&#10;left JOIN historico_interno h ON ( i.id_interno = h.idinterno)&#13;&#10;where I.ID_interno = :INTERNO                           &#13;&#10;        &#13;&#10;                                                          &#13;&#10;&#13;&#10;    &#13;&#10;&#13;&#10; &#13;&#10;    " Database="Database" PropData="05704C65667402580470546F70025C0A506172616D657465727301010C3F000000204E616D653D22494E5445524E4F222044617461547970653D226674537472696E67222045787072657373696F6E3D22636F6D626F626F78312E74657874220000"/>
