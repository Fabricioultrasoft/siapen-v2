    <TfrxDBXQuery Name="Sqlconfere" UserName="Sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT &#13;&#10;    CELA.CELA,&#13;&#10;   (SELECT INTERNO.NOME_INTERNO&#13;&#10;    FROM INTERNO                         &#13;&#10;    JOIN SP_RASTREAR_MOVIMENTACAO S ON (S.ID_INTERNO=INTERNO.ID_INTERNO)&#13;&#10;    WHERE&#13;&#10;    :DATA BETWEEN S.DATA AND S.ULTIMA_DATA -1 AND&#13;&#10;    INTERNO.ST='A' AND&#13;&#10;    COALESCE(INTERNO.EM_TRANSITO,'N')='N' AND        &#13;&#10;    INTERNO.idcela = CELA.id_cela) NOME_INTERNO,&#13;&#10;&#13;&#10;    NULL DATA_ENTRADA,&#13;&#10;    PAVILHAO.PAVILHAO,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    COALESCE((SELECT COUNT(*)              &#13;&#10;              FROM INTERNO                         &#13;&#10;                JOIN SP_RASTREAR_MOVIMENTACAO S ON (S.ID_INTERNO=INTERNO.ID_INTERNO)&#13;&#10;                WHERE&#13;&#10;                :DATA BETWEEN S.DATA AND S.ULTIMA_DATA -1 AND&#13;&#10;              INTERNO.ST='A' AND&#13;&#10;              COALESCE(INTERNO.EM_TRANSITO,'N')='N' AND                  &#13;&#10;              INTERNO.idcela = CELA.id_cela&#13;&#10;    ), 0) TOTAL_PISO&#13;&#10;FROM CELA&#13;&#10;   INNER JOIN SOLARIO ON (CELA.idsolario = SOLARIO.id_solario)&#13;&#10;   INNER JOIN GALERIA ON (solario.idgaleria = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN PAVILHAO ON (galeria.idpavilhao = PAVILHAO.ID_PAVILHAO)&#13;&#10;WHERE pavilhao.id_pavilhao=104       " Database="Database" PropData=""/>
