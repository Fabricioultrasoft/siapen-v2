    <TfrxDBXQuery Name="Sqlsemfoto" UserName="Sqlsemfoto" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select i.nome_interno, i.foto, p.pavilhao, g.galeria,&#13;&#10;c.cela, s.solario, n.nome_up&#13;&#10;&#13;&#10;&#13;&#10;from interno i&#13;&#10;&#13;&#10;&#13;&#10;   INNER JOIN PAVILHAO p ON (I.idpavilhao = P.id_pavilhao)&#13;&#10;   INNER JOIN GALERIA g ON (I.idgaleria = G.id_galeria)&#13;&#10;   INNER JOIN CELA c ON (I.idcela = C.id_cela)&#13;&#10;   INNER JOIN SOLARIO s ON (I.idsolario = S.id_solario)&#13;&#10;   INNER JOIN unidade_penal n ON (I.id_up = n.id_up)&#13;&#10;&#13;&#10;WHERE I.ST = 'A' AND I.ID_UP = :ID_UP&#13;&#10;AND i.foto is null&#13;&#10;&#13;&#10;order by i.nome_interno&#13;&#10;  &#13;&#10;&#13;&#10;      " Database="database" PropData="05704C65667402600470546F7003A4000A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
