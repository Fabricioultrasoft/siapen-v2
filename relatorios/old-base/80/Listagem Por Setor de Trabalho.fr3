    <TfrxIBXQuery Name="sqlsetortrabalho" UserName="sqlsetortrabalho" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select&#13;&#10;I.nome_interno, &#13;&#10;st.setor_trabalho,&#13;&#10;i.data_setor,&#13;&#10;p.pavilhao,&#13;&#10;g.galeria,&#13;&#10;c.cela,&#13;&#10;s.solario,  &#13;&#10;F.FUNCAO_INTERNO,  &#13;&#10;I.OBSTRABALHO                            &#13;&#10;from interno i&#13;&#10;join setor_trabalho st on (I.idsetor_trabalho=st.id_setor_trabalho)&#13;&#10;INNER JOIN PAVILHAO p ON (I.IDPAVILHAO = P.ID_PAVILHAO)&#13;&#10;iNNER JOIN GALERIA g ON (I.IDGALERIA = G.ID_GALERIA)&#13;&#10;INNER JOIN CELA c ON (I.IDCELA = C.ID_CELA)&#13;&#10;INNER JOIN SOLARIO s ON (I.IDSOLARIO = S.ID_SOLARIO)&#13;&#10;LEFT JOIN FUNCAO_INTERNO F ON (I.ID_FUNCAOINTERNO = F.ID_FUNCAO_INTERNO)    &#13;&#10;where st.setor_trabalho not like 'Z%' and i.st ='A' and st.setor_trabalho not like ''&#13;&#10;AND I.ID_UP = :ID_UP               &#13;&#10;and st.setor_trabalho LIKE :setor || '%'    &#13;&#10;order by st.setor_trabalho, i.nome_interno&#13;&#10;    " Database="IBXDatabase1" PropData=""/>
