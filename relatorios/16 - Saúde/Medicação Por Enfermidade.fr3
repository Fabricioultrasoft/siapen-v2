    <TfrxDBXQuery Name="sqlconfere" UserName="sqlconfere" CloseDataSource="True" FieldAliases.Text="" SQL.Text="  SELECT&#13;&#10;    INTERNO.NOME_INTERNO,&#13;&#10;    INTERNO.RGI,&#13;&#10;    ENFERMIDADE.enfermidade,&#13;&#10;    GALERIA.GALERIA,&#13;&#10;    CELA.CELA,&#13;&#10;    SOLARIO.SOLARIO,&#13;&#10;    (SELECT list(coalesce(remedio_enfermidade.qtde,'1')||' '||remedio.nome_remedio,', ')as remedio&#13;&#10;    from remedio&#13;&#10;    join remedio_enfermidade on (remedio.id_remedio=remedio_enfermidade.id_remedio)&#13;&#10;    join enfermdiade_interno on (remedio_enfermidade.id_enfermidade_interno=enfermdiade_interno.id_enfermdiade_interno)&#13;&#10;    where enfermdiade_interno.id_enfermidade=enfermidade.id_enfermidade&#13;&#10;    and coalesce(remedio_enfermidade.status,'')=''&#13;&#10;    and enfermdiade_interno.id_interno=interno.id_interno&#13;&#10;    and enfermidade.enfermidade like '%'|| :enfermidade ||'%')&#13;&#10;FROM INTERNO&#13;&#10;   join enfermdiade_interno on (interno.id_interno=enfermdiade_interno.id_interno)&#13;&#10;   join enfermidade on (enfermdiade_interno.id_enfermidade=enfermidade.id_enfermidade)&#13;&#10;   INNER JOIN GALERIA ON (INTERNO.IDGALERIA = GALERIA.ID_GALERIA)&#13;&#10;   INNER JOIN CELA ON (INTERNO.IDCELA = CELA.ID_CELA)&#13;&#10;   INNER JOIN SOLARIO ON (INTERNO.IDSOLARIO = SOLARIO.ID_SOLARIO)&#13;&#10;WHERE INTERNO.ST = 'A' AND INTERNO.ID_UP = :ID_UP and&#13;&#10;exists (SELECT null&#13;&#10;    from remedio&#13;&#10;    join remedio_enfermidade on (remedio.id_remedio=remedio_enfermidade.id_remedio)&#13;&#10;    join enfermdiade_interno on (remedio_enfermidade.id_enfermidade_interno=enfermdiade_interno.id_enfermdiade_interno)&#13;&#10;    where enfermdiade_interno.id_enfermidade=enfermidade.id_enfermidade&#13;&#10;    and coalesce(remedio_enfermidade.status,'')=''&#13;&#10;    and enfermdiade_interno.id_interno=interno.id_interno&#13;&#10;    and enfermidade.enfermidade like '%'|| :enfermidade ||'%')&#13;&#10;order by galeria.galeria, interno.nome_interno&#13;&#10;  &#13;&#10;&#13;&#10;                                                       " Database="database" PropData=""/>
    <TfrxDBXQuery Name="Sqlup" UserName="Sqlup" CloseDataSource="True" FieldAliases.Text="" SQL.Text="SELECT u.nome_up, u.endereco, u.bairro, u.numero, u.complemento, u.cep,&#13;&#10;c.cidade||' - '||c.uf as cidade, u.fone, u.fax,u.contato, u.email                     &#13;&#10;from unidade_penal u&#13;&#10;left join cidade c on(c.id_cidade=u.id_cidade)                                                       &#13;&#10;WHERE ID_UP = :ID_UP  " Database="database" PropData="05704C6566740300010470546F7002280A506172616D657465727301010C3F000000204E616D653D2249445F5550222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
    <TfrxDBXQuery Name="SQLDIRETOR" UserName="SQLDIRETOR" CloseDataSource="True" FieldAliases.Text="" SQL.Text="select f.nome_funcionario, u.nome_up, f.funcao from funcionario f&#13;&#10;inner join unidade_penal u on (u.id_up=f.id_up)&#13;&#10;where f.id_up = :id_up and f.funcao = 'DIRETOR'  " Database="database" PropData="05704C6566740300010470546F7002640A506172616D657465727301010C3F000000204E616D653D2269645F7570222044617461547970653D226674496E7465676572222045787072657373696F6E3D22262336303B49445F5550262336323B220000"/>
